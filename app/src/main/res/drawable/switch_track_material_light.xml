<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_enabled="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <nine-patch n1:src="@drawable/op_switch_track_mtrl_alpha" n1:tint="@color/switch_track_disable_color_light" />
    </item>
    <item n2:state_checked="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <nine-patch n2:src="@drawable/op_switch_track_mtrl_alpha" n2:tint="@color/setting_switch_on" n2:alpha="0.5" />
    </item>
    <item>
        <nine-patch n3:src="@drawable/op_switch_track_mtrl_alpha" n3:tint="@color/switch_track_normal_color_light"
          xmlns:n3="http://schemas.android.com/apk/res/android" />
    </item>
</selector>