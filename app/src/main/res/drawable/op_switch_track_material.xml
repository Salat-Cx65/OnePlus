<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_checked="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <layer-list>
            <item n1:gravity="fill_horizontal|center" n1:start="4.0dip" n1:end="4.0dip">
                <shape n1:tint="@color/op_switch_track_material" n1:shape="rectangle">
                    <corners n1:radius="7.0dip" />
                    <solid n1:color="@android:color/transparent" />
                    <stroke n1:width="2.0dip" n1:color="@color/switch_white_disabled_checked_material" />
                    <size n1:height="14.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item n2:state_checked="false"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <layer-list>
            <item n2:gravity="fill_horizontal|center" n2:start="4.0dip" n2:end="4.0dip">
                <shape n2:tint="@color/op_switch_track_material" n2:shape="rectangle">
                    <corners n2:radius="7.0dip" />
                    <solid n2:color="@android:color/transparent" />
                    <stroke n2:width="2.0dip" n2:color="@color/switch_white_disabled_material" />
                    <size n2:height="14.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>